  "bugs": {
    "url": "https://github.com/rizkiadi23/sirclo_berat_app/issues"
  },
  "homepage": "https://github.com/rizkiadi23/sirclo_berat_app#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.2.7"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  }
}
